{
  "name": "ltcat",
  "version": "1.0.0-alpha.1",
  "packageManager": "pnpm@8.6.11",
  "repository": {
    "type": "git",
    "url": "https://github.com/AliSajid/ltcat.git"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "pnpm run test:integration && pnpm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . --plugin 'prettier-plugin-svelte' && eslint .",
    "format": "prettier . --write . --plugin 'prettier-plugin-svelte'",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "emulate": "firebase emulators:start --export-on-exit=./emulator-data --import=./emulator-data",
    "release": "pnpx semantic-release",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@storybook/addon-a11y": "^7.2.1",
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-interactions": "^7.2.1",
    "@storybook/addon-jest": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/addon-storyshots": "^7.2.1",
    "@storybook/addon-styling": "^1.3.5",
    "@storybook/blocks": "^7.2.1",
    "@storybook/svelte": "^7.2.1",
    "@storybook/sveltekit": "^7.2.1",
    "@storybook/testing-library": "^0.2.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.5.1",
    "eslint": "^8.46.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.5",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-svelte": "^2.32.4",
    "event-emitter": "^0.3.5",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "prettier-plugin-svelte": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^21.0.7",
    "storybook": "^7.2.1",
    "svelte": "^4.1.2",
    "svelte-check": "^3.4.6",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vercel": "^31.2.2",
    "vite": "^4.4.8",
    "vitest": "^0.32.4"
  },
  "type": "module",
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "dependencies": {
    "@sveltejs/adapter-vercel": "^3.0.3",
    "firebase": "^9.23.0"
  }
}
